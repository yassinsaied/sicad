{% extends 'admin/baseBack.html.twig' %}

{% block title %}
	{{'add.article'|trans}}
{% endblock %}


{% block content %}

	<div class="col-xl-10">
		<div class="card">
			<div class="card-header">
				<div class="row align-items-center">
					<div class="col-8">
						<h3 class="mb-0"></h3>
					</div>

				</div>
			</div>


			<div class="card-body"></div>


