			{{ form_widget(form , {'attr': {'required': false}}) }}
			{# {{ form_errors(form) }} #}
		</div>
	</div>
{% endblock %}

{% block javascripts %}

	<script src="{{asset('js/multIUploadArticel.js')}}"></script>

{% endblock %}
